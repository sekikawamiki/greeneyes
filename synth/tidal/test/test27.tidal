--cps 1

bps (120/60)

d1 $ s "bd" # gain "0.8 0.3 0.6 0.7"

d1 silence

d2 $ gain "0.8 1 0.6 0.7" # s "bd"

d2 silence

d3 $ s "arpy*4" # speed "-1 1 0.8 4"

d3 silence

d4 $ speed "-1 1 0.8 4" # s "arpy"

d4 silence

d5 $ s "arpy*4" # speed "[1, 1.5, 1.5 2 3 4]"

d5 silence

d6 $ slow 8 $ s "arpy*12" # up (run 12)

d6 silence

d7 $ slow 8 $ s "arpy arpy arpy:1 arpy:2 arpy:4 arpy:4 arpy:2"

d7 silence

d8 $ s "bd(3,8,2)"

d8 silence

d9 $ slow 4 $ s "bd([3 5]/2,8)"

d9 silence

hush
